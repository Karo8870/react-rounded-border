    "@swc/core": "^1.10.4",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@turf/helpers": "^7.2.0",
    "@turf/union": "^7.2.0"
  }
